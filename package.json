{
  "name": "my-fetch-plus",
  "version": "0.0.2",
  "author": "webszy",
  "homepage": "https://github.com/webszy/fetch-plus",
  "keywords": [
    "ajax",
    "request",
    "fetch",
    "fetch-plus"
  ],
  "description": "enhance Fetch API experience",
  "main": "dist/FetchPlus.es.js",
  "type": "module",
  "module": "es/index.js",
  "browser": "dist/FetchPlus.umd.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "es/index.js",
      "require": "lib/index.js"
    },
    "./*": "./*"
  },
  "repository": "https://github.com/webszy/fetch-plus.git",
  "bugs": "https://github.com/webszy/fetch-plus/issues",
  "license": "MIT",
  "scripts": {
    "dev": "yarn run build && copyfiles ./dist/*.* ./demo && http-server ./demo",
    "build": "rimraf dist && tsc && vite build",
    "publish": "npm publish --access=public",
    "docs:dev": "vitepress dev ./docs",
    "docs:build": "vitepress build ./docs",
    "docs:preview": "vitepress preview ./docs"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "private": false,
  "dependencies": {
    "copyfiles": "^2.4.1",
    "http-server": "^14.1.1",
    "vitepress": "^1.0.0-alpha.65"
  }
}
